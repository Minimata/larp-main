  "lint-staged": {
    "*.{js,vue,ts}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.4",
    "@nuxtjs/pwa": "^3.3.5",
    "commitizen": "^4.2.5",
    "daisyui": "^2.22.0",
    "firebase": "^9.9.3",
    "firebase-admin": "^11.0.1"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.0.0-rc.3",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@pinia/nuxt": "^0.4.1",
    "@vueuse/nuxt": "^9.1.0",
    "axios": "^0.27.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "nuxt": "3.0.0-rc.1",
    "pinia": "^2.0.13",
    "postcss": "^8.4.16",
    "postcss-custom-properties": "^12.1.8",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "^3.0.24",
    "testcafe": "^1.18.6",
    "typescript": "^4.6.4",
    "vitest": "^0.22.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
